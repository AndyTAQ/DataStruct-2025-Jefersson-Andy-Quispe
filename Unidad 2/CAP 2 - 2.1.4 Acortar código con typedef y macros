#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef vector<int> vi;
typedef pair<int,int> pi;
#define F first
#define S second
#define PB push_back
#define MP make_pair
#define REP(i,a,b) for (int i = a; i <= b; i++)
int main() {
vi v;
v.PB(1); v.PB(2); v.PB(3);
for (auto x : v) cout << x << " ";
cout << "\n";
REP(i,1,5) cout << i << " ";
cout << "\n";
return 0;
}
